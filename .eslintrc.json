{
    "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb", "prettier"],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": "error",
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-console": "warn",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    }
  }

